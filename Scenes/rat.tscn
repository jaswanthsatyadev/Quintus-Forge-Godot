const gravity = 900
var knockback_force = 200
var is_roaming: bool = true

func _ready():
	is_rat_chase = false

func choose(array):
	array.shuffle()
	return array.front()
	
func _on_timer_timeout():
	$Timer.wait_time = choose([1.5,2,2.5])
	if !is_rat_chase:
